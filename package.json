{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier:check": "prettier --check \"frontend/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "prettier:fix": "prettier --write \"frontend/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "eslint:check": "eslint --ext ts,tsx,js,jsx .",
    "eslint:fix": "eslint --ext ts,tsx,js,jsx . --fix",
    "import-sort:check": "import-sort --list-different \"frontend/**/*.{js,jsx,ts,tsx}\"",
    "import-sort:fix": "import-sort --write \"frontend/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run prettier:check && yarn run eslint:check",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.4",
    "@mantine/form": "^6.0.4",
    "@mantine/hooks": "^6.0.4",
    "@mantine/modals": "^6.0.4",
    "@mantine/notifications": "^6.0.4",
    "@tabler/icons-react": "^2.11.0",
    "@types/react-router-dom": "5.3.3",
    "classnames": "^2.3.2",
    "invariant": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/ui": "^0.29.8",
    "autoprefixer": "^10.4.13",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.29.8"
  }
}

{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build:development": "tsc && vite build",
    "build:production": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "prettier:check": "prettier --check \"frontend/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "prettier:fix": "prettier --write \"frontend/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "eslint:check": "eslint --ext ts,tsx,js,jsx .",
    "eslint:fix": "eslint --ext ts,tsx,js,jsx . --fix",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "lint": "npm run prettier:check && npm run eslint:check && npm run tsc:check",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "openapi:generate": "node ./frontend/utils/generate-types.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.5",
    "@mantine/form": "^6.0.5",
    "@mantine/hooks": "^6.0.5",
    "@mantine/modals": "^6.0.5",
    "@mantine/notifications": "^6.0.5",
    "@tabler/icons-react": "^2.11.0",
    "@types/luxon": "^3.3.3",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-router-dom": "5.3.3",
    "ajv": "^8.12.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "humps": "^2.0.1",
    "invariant": "^2.2.4",
    "luxon": "^3.4.3",
    "mantine-react-table": "^1.0.0-beta.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.10.0",
    "swr": "^2.1.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/humps": "^2.0.2",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "autoprefixer": "^10.4.13",
    "eslint": "8.38.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "json-schema-to-zod": "^1.2.0",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
